<h1> Advanced Search</h1>

<%= form_for @search do |f| %>
    <div class="field">
        <%=f.label :first_name %><br />
        <%=f.text_field :first_name %>
    </div>
    
    <div class="field">
        <%=f.label :last_name %><br />
        <%=f.text_field :last_name %>
    </div>
    
    </div class = "actions"><%=f.submit "Search" %></div>
    
<% end %>
    
        